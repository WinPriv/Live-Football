<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/hiad_video_progressbar_h"/>
            <solid android:color="@color/hiad_video_progress_bg"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/hiad_video_progressbar_h"/>
                <solid android:color="@color/hiad_video_progress_buffer"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/hiad_video_progressbar_h"/>
                <solid android:color="@color/hiad_video_progress_blue_hm"/>
            </shape>
        </clip>
    </item>
</layer-list>
