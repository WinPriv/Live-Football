<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="@color/applovin_sdk_consentFlowGdprSwitchThumb"/>
            <size
                android:height="36dp"
                android:width="36dp"/>
            <stroke
                android:width="2dp"
                android:color="@color/applovin_sdk_consentFlowGdprSwitchTrackDisabled"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/applovin_sdk_consentFlowGdprSwitchThumb"/>
            <size
                android:height="36dp"
                android:width="36dp"/>
            <stroke
                android:width="2dp"
                android:color="@color/applovin_sdk_consentFlowGdprAccentColor"/>
        </shape>
    </item>
</selector>
